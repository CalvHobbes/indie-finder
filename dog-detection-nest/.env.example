# Roboflow API Configuration
# Get your API key from Roboflow dashboard
ROBOFLOW_API_KEY=your_roboflow_api_key_here
# Roboflow API endpoint for your workflow
ROBOFLOW_API_URL=https://serverless.roboflow.com/infer/workflows/your_workflow_id/your_workflow_name

# Server Configuration
PORT=5001
# Comma-separated list of allowed CORS origins
CORS_ORIGIN=http://localhost:3000

# Debugging
# Set to 'true' to enable debug logging to logs/roboflow-debug.log
DEBUG_MODE=false
